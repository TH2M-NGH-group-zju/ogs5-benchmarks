    }
  ERRORBARS
    {
    COLOR = RED
    }
$!XYLINEAXIS 
  DEPXTOYRATIO = 1
  GRIDAREA
    {
    DRAWBORDER = YES
    }
$!XYLINEAXIS 
  XDETAIL 1
    {
    RANGEMIN = 0
    RANGEMAX = 1000
    GRSPACING = 200
    TICKLABEL
      {
      TEXTSHAPE
        {
        HEIGHT = 4
        }
      }
    TITLE
      {
      TITLEMODE = USETEXT
      TEXT = 'X (m)' 
      TEXTSHAPE
        {
        HEIGHT = 4.6
        }
      OFFSET = 5
      }
    AXISLINE
      {
      LINETHICKNESS = 0.6
      }
    }
$!XYLINEAXIS 
  YDETAIL 1
    {
    RANGEMIN = 70
    RANGEMAX = 120
    GRSPACING = 10
    TICKLABEL
      {
      TEXTSHAPE
        {
        HEIGHT = 4
        }
      LABELALIGNMENT = ALONGAXIS
      }
    MINORGRIDLINES
      {
      COLOR = CUSTOM2
      LINEPATTERN = SOLID
      LINETHICKNESS = 1
      }
    TITLE
      {
      TITLEMODE = USETEXT
      TEXT = 'Heat loss in °C without solid matrix' 
      TEXTSHAPE
        {
        HEIGHT = 4.6
        }
      OFFSET = 4
      }
    AXISLINE
      {
      LINETHICKNESS = 0.6
      }
    }
$!XYLINEAXIS 
  YDETAIL 2
    {
    RANGEMIN = 119.2
    RANGEMAX = 120
    AUTOGRID = NO
    GRSPACING = 0.2
    TICKLABEL
      {
      TEXTSHAPE
        {
        HEIGHT = 4
        }
      LABELALIGNMENT = ALONGAXIS
      }
    TITLE
      {
      TITLEMODE = USETEXT
      TEXT = 'Heat loss in °C with solid matrix' 
      TEXTSHAPE
        {
        HEIGHT = 4.6
        }
      OFFSET = 4
      }
    AXISLINE
      {
      AXISALIGNMENT = WITHGRIDMAX
      }
    }
$!LINEPLOTLAYERS 
  SHOWSYMBOLS = YES
$!SETSTYLEBASE CONFIG
